//7.A

for(int input = 20; input >= 0; input = input - 1){
  
  if (input == 6){ 
    println("six");
  } else if(input == 10){
    println("HALF!");
  } else {
    println(input);
  }
}

//7.B
for(int input = -20; input <= 0; input = input + 1){
  
  if (input == -5){ 
    println("Minus five!");
  } else if(input == -10){
    println("HALF!");
  } else {
    println(input);
  }
}
